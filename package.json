{
  "name": "react-with-vue-cli",
  "version": "0.1.0",
  "description": "An boilerplate use `vue-cli` to build `react-app`",
  "keywords": [
    "react",
    "create-react-app",
    "vue-cli",
    "@vue/cli"
  ],
  "homepage": "https://github.com/VdustR/react-with-vue-cli",
  "bugs": {
    "url": "https://github.com/VdustR/react-with-vue-cli/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ViPro",
    "email": "VdustR@gmail.com",
    "url": "https://VdustR.github.io"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx",
    "gh": "cross-env BASE_URL=/react-with-vue-cli/ yarn build ; gh-pages -d dist -b gh-pages"
  },
  "devDependencies": {
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "gh-pages": "^2.0.1",
    "vue-template-compiler": "^2.5.19"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "^2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "airbnb"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
